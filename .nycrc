{
    "description": "",
    "reporter": [
        "html",
        "text",
        "clover"
    ],
    "include": [
      "test/*.js",
      "test/*.mjs",
      "app.js",
      "app.mjs",
      "routes/*.mjs",
      "routes/*.js",
      "src/*.mjs",
      "src/*.js"
    ],
    "exclude": [
      "coverage/",
      "mongo-entrypoint"
    ],
    "all": true,
    "useSpawnWrap": true,
    "report-dir": "./build/coverage",
    "temp-directory": "./build/.nyc_output"
}